{
  "name": "tslib-starter",
  "version": "0.0.1-SNAPSHOT",
  "description": "",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc -w -p .",
    "build": "npm run cleanup && tsc ",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "cleanup": "rimraf .cache .tmp  build coverage dist",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@types/clear": "^0.1.4",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.1.0",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "figlet": "^1.7.0",
    "inquirer": "^10.1.8",
    "ora": "^8.0.1",
    "shelljs": "^0.8.5",
    "zx": "^8.1.4"
  }
}
